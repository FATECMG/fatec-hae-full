{
  "name": "aws-serverless-typescript-api",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "build": "tsup src/docker/main/index.ts",
    "run:docker": "node dist/index.js",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "cloud:deploy": "serverless deploy",
    "cloud:remove": "serverless remove",
    "lint": "npx eslint .",
    "lint:fix": "yarn lint --fix",
    "precommit": "npx lint-staged",
    "commit": "git commit -m"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.386.0",
    "@aws-sdk/client-s3": "^3.451.0",
    "@aws-sdk/s3-request-presigner": "^3.451.0",
    "@aws-sdk/util-dynamodb": "^3.303.0",
    "@middy/core": "^3.4.0",
    "@middy/http-header-normalizer": "^4.2.8",
    "@middy/http-json-body-parser": "^3.4.0",
    "aws-sdk": "^2.1347.0",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "emoji-regex": "^10.2.1",
    "express": "^4.18.2",
    "inversify": "^6.0.1",
    "mongoose": "^7.0.4",
    "nodemailer": "^6.9.3",
    "reflect-metadata": "^0.1.13",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node": "^14.14.25",
    "@types/nodemailer": "^6.4.8",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "esbuild": "^0.14.11",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.4",
    "json-schema-to-ts": "^1.5.0",
    "lint-staged": "^13.2.2",
    "mongodb-memory-server": "^8.12.2",
    "serverless": "^3.0.0",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-esbuild": "^1.43.0",
    "serverless-offline": "^12.0.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
